  <script>
    jQuery(document).on('change','.input-desktop,.input-mobile',function() {
      var qty = jQuery(this).val();
      var variant_id = jQuery(this).data('item_id');
      jQuery.ajax({
        type: 'POST',
        url: '/cart/change.js',
        dataType: 'json',
        data: {
          quantity: qty,
          id: variant_id
        },
        success: function(cart) {
          jQuery('.total_count').html(theme.Currency.formatMoney(cart.total_price, theme.settings.moneyFormat));
        },
        error: function(response) {
          console.log(response);
        }
