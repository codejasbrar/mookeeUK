    <script>

      var variant_line = [];
      {% for line in checkout.line_items %}
      {% assign product_delivery = line.product.metafields.delivery_days.delivery_days | times: 1 %}
      {% assign mattres_delivery = shop.metafields.mattress.delivery_days | times: 1 %}
      {% if product_delivery >= mattres_delivery %}
      variant_line.push("{{line.product.metafields.delivery_days.delivery_days}}");
                        {% else %}
                        {% if line.properties['Mattress Type'] != blank %}
                        variant_line.push("{{shop.metafields.mattress.delivery_days}}");
                        {% elsif line.product.title == "Mattress Type" %}
                        {% if product_delivery >= mattres_delivery %}
                        variant_line.push("{{line.product.metafields.delivery_days.delivery_days}}");
                        {% else %}
                        variant_line.push("{{shop.metafields.mattress.delivery_days}}")
                                          {% endif %}              
                        {% else %}
                        {% if line.variant.metafields.variant_date.variant <= "0" or line.variant.metafields.variant_date.variant == blank %}      
                        variant_line.push("{{line.product.metafields.delivery_days.delivery_days}}");
                        {% else %}
                        variant_line.push("{{ line.variant.metafields.variant_date.variant }}")
                        {% endif %}              
                        {% endif %}              
                        {% endif %}              

                        {% endfor %}  
                        var count = 0;
                        jQuery('.product__description__variant').each(function(){
        jQuery(this).append('<p class="variant_date">{{shop.metafields.delivery_text.shop}} <strong style="color:#000">'+variant_line[count]+'</strong> Days</p>');
                            count++;
                            });

        jQuery(".click_button").click(function() {
          jQuery(".collection_f_custom").slideToggle();
        }); 
        jQuery(".select_lang").click(function() {
