        jQuery(document).on('page:load', function() {
          jQuery( '.field[data-address-field="phone"], .field[data-address-field="city"]' ).addClass( 'field--half' ).removeClass('element-toggler');
          jQuery( '.field[data-address-field="phone"]' ).insertAfter( '.field[data-address-field="city"]' );
